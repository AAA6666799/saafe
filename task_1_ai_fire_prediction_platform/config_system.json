{
  "system_name": "synthetic_fire_prediction_system",
  "version": "0.1.0",
  "environment": "development",
  "max_processing_latency_ms": 100,
  "max_prediction_latency_ms": 500,
  "batch_size": 32,
  "num_workers": 4,
  "thermal_image_resolution": [384, 288],
  "feature_vector_size": 20,
  "sequence_length": 10,
  "model_accuracy_threshold": 0.90,
  "false_positive_threshold": 0.05,
  "false_negative_threshold": 0.01,
  "confidence_threshold": 0.8,
  "agent_communication_timeout": 5,
  "agent_retry_attempts": 3,
  "sensor_health_check_interval": 30,
  "calibration_check_interval": 3600
}