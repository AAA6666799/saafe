    "\n",
    "print(\"üìä TRAINING ML ENSEMBLE\")\n",
    "print(\"=\" * 30)\n",
    "\n",
    "# Random Forest\n",
    "print(\"üå≥ Training Random Forest...\")\n",
    "rf_model = RandomForestClassifier(\n",
    "    n_estimators=200, max_depth=15, random_state=42, n_jobs=-1\n",
