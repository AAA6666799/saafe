    "# Training loop\n",
    "num_epochs = 20\n",
    "best_val_loss = float('inf')\n",
    "train_losses = []\n",
    "val_losses = []\n",
    "fire_aucs = []\n",
    "fire_accuracies = []\n",
    "lead_time_accuracies = []\n",
    "\n",
    "print(f\"üî• Starting Fire Detection Training for {num_epochs} epochs...\")\n",
